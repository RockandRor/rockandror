<section class="page greetings">
  <div class="row header-bg">
    <div class="col-xs-10 col-xs-offset-1 col-lg-8 col-lg-offset-2">
      <h1>
        <%= t'pages.contact.greetings.title_html' %>
      </h1>
      <%= render 'shared/logo_rails' %>
    </div>
  </div>
  <div class="col-md-12">
    <div class="col-xs-10 col-xs-offset-1">
      <br/>
      <p class="text-center">
        <%= t'pages.contact.greetings.share.text_html' %>
        <br/>
        <div class="fb-like"
              data-href="https://www.facebook.com/rocknroragile/"
              data-layout="button"
              data-action="like"
              data-size="large"
              data-show-faces="false"
              data-share="false">
        </div>
      </p>
    </div>
  </div>
</section>
