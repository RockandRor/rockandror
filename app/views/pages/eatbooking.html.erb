<% title t'pages.works.eatbooking.meta.title' %>
<% meta_tag :description, t('pages.works.eatbooking.meta.description') %>
<% meta_tag :keywords, t('pages.works.eatbooking.meta.keywords') %>

<section class="page eatbooking">
  <div class="row header-bg">
    <div class="col-xs-10 col-xs-offset-1 col-lg-8 col-lg-offset-2">
      <%= link_to t('common.btn_back'), url_for(:back), class: 'btn-back' %>
      <h1>
        <%= t'pages.works.eatbooking.h1_title_html' %>
      </h1>
      <lead><%= t'pages.works.eatbooking.lead' %></lead>
      <%= render 'shared/logo_rails' %>
    </div>
  </div>

  <div class="row">
    <div class="col-xs-10 col-xs-offset-1 col-lg-8 col-lg-offset-2">
      <div class="row">
        <div class="col-md-3">
          <%= image_tag "eatbooking/logo.png", class: 'img-responsive' %>
        </div>
        <div class="col-md-9">
          <p>
            <%= t'pages.works.eatbooking.general_description_html' %>
          </p>
          <%= link_to 'Visita Eatbooking', 'http://eatbooking.com', class:'btn btn-lg btn-primary', target: 'blank' %>
        </div>
      </div>
      <hr />
      <br />
      <h2><%= t'pages.works.eatbooking.h2_1_html' %></h2>
      <p><%= t'pages.works.eatbooking.description_1_html' %></p>
      <br />
      <%= image_tag "eatbooking/cover-page.jpg", class: 'img-responsive' %>
      <br />
      <h2><%= t'pages.works.eatbooking.h2_2_html' %></h2>
      <p><%= t'pages.works.eatbooking.description_2_html' %></p>
      <hr />
      <h2><%= t'pages.works.eatbooking.h2_3_html' %></h2>
      <ul class="no-padding-left">
        <%= t'pages.works.eatbooking.list_3_html' %>
      </ul>
    </div>
    <div class="col-xs-12 accounting bg-grey-light">
      <div class="col-lg-10 col-lg-offset-1">
        <div class="col-xs-12 col-sm-4 text-center">
          <span class="glyphicon glyphicon-grain gi-2x"></span>
          <br />
          <br />
          <span class="text-uppercase">
            <%= t'common.accounting.delivery_time' %>
          </span>
          <br />
          <br />
          <strong>3 Meses</strong>
          <hr class="visible-xs">
        </div>
        <div class="col-xs-12 col-sm-4 text-center">
          <span class="glyphicon glyphicon glyphicon-time gi-2x"></span>
          <br />
          <br />
          <span class="text-uppercase">
            <%= t'common.accounting.hours_worked' %>
          </span>
          <br />
          <br />
          <strong>500</strong>
          <hr class="visible-xs">
        </div>
        <div class="col-xs-12 col-sm-4 text-center">
          <span class="glyphicon glyphicon glyphicon-euro gi-2x"></span>
          <br />
          <br />
          <span class="text-uppercase">
            <%= t'common.accounting.budget' %>
          </span>
          <br />
          <br />
          <strong>
            <%= t'common.accounting.medium' %>
          </strong>
        </div>
      </div>
      <div class="col-xs-12 col-md-10 col-md-offset-1 col-lg-10 col-lg-offset-1">
        <hr />
        <p class="text-center">
          <small>
            <%= t'common.accounting.guarantee_html' %>
          </small>
        </p>
      </div>
    </div>
  </div>

  <div class="row back-end">
    <div class="col-xs-10 col-xs-offset-1 col-lg-8 col-lg-offset-2">
      <h2><%= t'pages.works.eatbooking.h2_6_html' %></h2>
      <p><%= t'pages.works.eatbooking.description_6_html' %></p>
      <%= image_tag "eatbooking/eatbooking-ipad.png", class: 'img-responisve' %>
      <br />
    </div>
  </div>

  <div class="row tecnology">
    <div class="col-xs-10 col-xs-offset-1 col-lg-8 col-lg-offset-2">
      <h2><%= t'pages.works.eatbooking.h2_tecnology_html' %></h2>
      <p><%= t'pages.works.eatbooking.tecnology_html' %></p>
    </div>
  </div>
  <%= testimonials(
                  'testimonials/boatjump.jpg',
                  'CEO: Francisco de la Mata',
                  (t'pages.works.eatbooking.customer_description_html'))
                  %>
</section>

<%= render 'shared/contact_now' %>
